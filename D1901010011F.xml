function fn_CommonSearch()
{
	fn_D1_Find();
}
//------------------------------------------------------------------------------- 
// 함수명: fn_D1_Find()
//   설명:  배관탐사이력을 조회한다.
// arguments
//   return 내용: 없음
//------------------------------------------------------------------------------- 
function fn_D1_Find()
{	
	if(!fn_DateCheck(cal_ExplorYmdFrom,cal_ExplorYmdTo))
	{
		return;
	}
	
	if(!fn_DateCheckToday(cal_ExplorYmdTo))
	{
		return;
	}
	
	var strSvcID        = "fn_D1_Find";
	var strPart         = "D10";	
	var strURL          = "handle.do?ServiceName=D1_PipingSechHistorySearMgr-service" 
						+ "&BRANCH_CD="      + cob_BranchCd.Value
						+ "&ZIP_NO1="        + cob_CountyFlag.Value
						+ "&EXPLOR_YMDFROM=" + cal_ExplorYmdFrom.Value
						+ "&EXPLOR_YMDTO="   + cal_ExplorYmdTo.Value;
	var strInDatasets   = "";
	var strOutDatasets  = "ds_PipeRst=ds_PipeRst";
	var strArgument     = "find";
	
	gfn_SyncCall( 
					 strSvcID
				   , strPart
				   , strURL
				   , strInDatasets
				   , strOutDatasets
				   , strArgument
			   );
	if(ds_PipeRst.GetRowCount()==0)
        Grid2.NoDataText=gfn_GetMessage("M000009","%AAA%","조회를");
}

