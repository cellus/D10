//-------------------------------------------------------------------------------
function fn_D1_Find()
{
	var strSvcID = "fn_D1_Find";																																																									
	var strPart = "D10";				
	var strURL = "handle.do?ServiceName=D1202010015U-service"
	           + "&FACI_NUM="				+strFaciNum			//밸브박스번호
	           + "&VALVE_NUM="				+strValveNum		//밸브번호
	           + "&PATROL_YMD=" 			+strPatrolYmd		//순찰일자
	           ;
	var strInDatasets  = "";
	var strOutDatasets  = "ds_ValveList=ds_ValveList";
	var strArgument  = "find";
	
	gfn_SyncCall(strSvcID,
	              strPart,
	              strURL,
	              strInDatasets,																						
	              strOutDatasets,
	              strArgument);
	
	if(ds_ValveList.GetRowCount()==0)
	{
	
	}

	fn_D1_Set();
}


//------------------------------------------------------------------------------- 
// 함수명: fn_D1_Set()
//   설명:  순찰중점관리 교량첨가배관 점검대장 해당 내용을 셋팅한다.
// arguments
//   return 내용: 없음
//------------------------------------------------------------------------------- 
function fn_D1_Set()
{	
	for(var i=0; i<ds_ValveList.RowCount(); i++)
	{	
		if(ds_ValveList.GetColumn(i,"CHK_RSLT") == "1")			//점검결과
		{
			ds_Valve.SetColumn(i,"CHK_PASS",1);					//양호
		}
		else if(ds_ValveList.GetColumn(i,"CHK_RSLT") == "2")	//점검결과
		{
			ds_Valve.SetColumn(i,"CHK_NOT_PASS",1);				//불량
		}
		ds_Valve.SetColumn(i,"RMK1",ds_ValveList.GetColumn(i,"RMK1"));	//비고
		ta_Rmk2.Text = ds_ValveList.GetColumn(i,"RMK2");				//특기사항
	}
}


//---------------------------------------------------------
// 검사 결과 에 따른 체크값 설정(중복체크 안되게 함)
//---------------------------------------------------------
function grd_Multi_OnCellClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	if("CHK_PASS" == grd_Multi.GetCellProp("body",nCell,"ColId"))
	{
		ds_Valve.SetColumn(nRow,"CHK_NOT_PASS",0);
	}else if("CHK_NOT_PASS" == grd_Multi.GetCellProp("body",nCell,"ColId"))
	{
		ds_Valve.SetColumn(nRow,"CHK_PASS",0);
	}
}

//------------------------------------------------------------------------------- 
//   함수명: fn_D1_SAVE
//   설명  : 사유지 점용료 협의사항를 등록/업데이트한다.
//   arguments
//   return 내용: 없음
//------------------------------------------------------------------------------- 
function fn_D1_Save()
{	
	if(ds_Valve.RowCount() == 0)
	{
		return;
	}

	var strType = "";
	
	for(var i=0; i<ds_Valve.RowCount(); i++)
	{
		var strRstCd = 0;
		if(ds_Valve.GetColumn(i,"CHK_PASS") == 1)
		{
			strRstCd = 1;
		}else if(ds_Valve.GetColumn(i,"CHK_NOT_PASS") == 1)
		{
			strRstCd = 2;
		}
		// else 
		// {
			// alert('검사항목을 모두 체크해 주세요');
			// return;
		// }
		ds_Valve.UpdateControl = false;
		if(ds_ValveList.GetRowCount()>0)
		{
			ds_Valve.SetRowType(i, "update");
		}
		else
		{
			ds_Valve.SetRowType(i, "insert");
		}
		// ds_Valve.UpdateControl = false;
		// ds_Valve.SetRowType(i, strType);
		
		//수정 등록시 셋팅할 사용자 정보
		ds_Valve.SetColumn(i, "FACI_NUM", strFaciNum);			//밸브박스번호
		ds_Valve.SetColumn(i, "VALVE_NUM", strValveNum);		//밸브번호
		ds_Valve.SetColumn(i, "PATROL_YMD", strPatrolYmd);		//순찰일자
		ds_Valve.SetColumn(i, "CHK_RSLT", strRstCd);			//점검결과
		//ds_Valve.SetColumn(i, "RMK2", ta_Rmk2.Text);			//특기사항
		
		
		ds_Valve.SetColumn(i, "UPD_EMPID", gfn_GetGlobalValue("G_USER_NO"));
		ds_Valve.SetColumn(i, "UPD_IP", gfn_GetGlobalValue("G_IPADDRESS"));
		ds_Valve.SetColumn(i, "CRT_EMPID", gfn_GetGlobalValue("G_USER_NO"));
		ds_Valve.SetColumn(i, "CRT_IP", gfn_GetGlobalValue("G_IPADDRESS"));
	}
	
	ds_ValveRmk.AddRow();
	ds_ValveRmk.UpdateControl=false;
	ds_ValveRmk.SetColumn(0,"FACI_NUM",strFaciNum);
	ds_ValveRmk.SetColumn(0,"VALVE_NUM",strValveNum);
	ds_ValveRmk.SetColumn(0,"PATROL_YMD",strPatrolYmd);
	ds_ValveRmk.SetColumn(0,"RMK2",ta_Rmk2.Text);
	ds_ValveRmk.SetColumn(0,"UPD_EMPID",gfn_GetGlobalValue("G_USER_NO"));
	ds_ValveRmk.SetColumn(0,"UPD_IP",gfn_GetGlobalValue("G_IPADDRESS"));
	ds_ValveRmk.SetRowType(0,"update");
	
	var strSvcID         = "fn_D1_Save";
	var strPart          = "D10";	
	var strURL           = "handle.do?ServiceName=D1202010015U-service" ;					
	var strInDatasets    = "ds_Valve=ds_Valve:u ds_ValveRmk=ds_ValveRmk:u";
	var strOutDatasets   = "";
	var strArgument      = "save";
	
	gfn_AsyncCall( 
					 strSvcID
				   , strPart  	 
				   , strURL
				   , strInDatasets
				   , strOutDatasets
				   , strArgument
			   );
			   
	if(ErrorCode < 0)
	{   
		Close("saveNo");
		return;
	}else
	{
		alert(gfn_GetMessage("M000009","%AAA%","저장을"));
	}
	
	Close("saveOk");
}


