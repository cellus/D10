	<Script><![CDATA[/***************************************************************************************************
* FormID     : D1802010010U.xml
* Form 설명  : 도로상 점용과세표
* 시스템 명  : 안전공급 (D10)
* 작성자     : 정주혜 
* 작성일     : 2009-02-01
* 변경내용   : 
****************************************************************************************************/

#include "JS::COMMONA10001.js";
#include "JS::COMMONA10002.js";
#include "JS::COMMOND10001.js";

var nCount;

function D18020010U_OnLoadCompleted(obj)
{
	gfn_LoadForm(obj);
	fn_Init();
}                                                                 

//---------------------------------------------------------
//	초기 설정
//---------------------------------------------------------
function fn_Init()
{	
	//fn_SetButton("0011011");
	cob_BaseYear.Value = subStr(today(), 0, 4);
	Static10.Text      = subStr(today(), 0, 4);
	
	fn_CommonSearch();
}

/*===============================================================================
 * 공통 div_ComHead 에서 버튼 이벤트
		추가	fn_CommonAddRow
		삭제	fn_CommonDeleteRow
		저장	fn_CommonSave
		조회	fn_CommonSearch
		엑셀	fn_CommonExcelExport
		인쇄	fn_CommonPrint
		닫기	공통에서 처리
===============================================================================*/ 
function fn_CommonSearch(){

	Static10.Text = cob_BaseYear.Text;
	
	nCount = fn_D1_check();
	fn_D1_find_A();
}

function fn_CommonAddRow(){	
	
	if(ds_Tax_A.GetRowCount() > 0){
		return;
	}
	
	if(ds_Tax_B.GetRowCount() > 0){
		return;
	}
	
	if(ds_Tax_C.GetRowCount() > 0){
		return;
	}
		
	addNum = 1;			
		
	ds_Tax_A.Copy("ds_Tax_A_New");
	
	if(cob_BaseYear.Value <= '2005'){
		ds_Tax_B.Copy("ds_Tax_B_Old");
	}else{
		for(var i = ds_Tax_B_New.GetColumn(ds_Tax_B_New.GetRowCount()-1, "INSTALL_YESR"); i<cob_BaseYear.Value; i++){
			
			var j = ds_Tax_B_New.AddRow();
			
			ds_Tax_B_New.SetColumn(j, "INSTALL_YESR"   , parseInt(i)+1       );
			ds_Tax_B_New.SetColumn(j, "INSTALL_YESR_NM", parseInt(i)+1 + "년");	
		}
	
		ds_Tax_B.Copy("ds_Tax_B_New");
	}
		
	ds_Tax_C.AddRow();
	ds_Tax_C.SetColumn(0, "BASE_YEAR"     , cob_BaseYear.Value);
} 

function fn_CommonSave(){
	
	if(ds_Tax_A.GetColumn(0, "UNIT_PRC") == '' || ds_Tax_A.GetColumn(0, "UNIT_PRC") == null){
		alert(gfn_GetMessage("M000015","%AAA%","과표 금액(원)은 "));
		return;
	}
	
	fn_D1_save_A();
	fn_D1_save_B();
	fn_D1_save_C();
	
	alert(gfn_GetMessage("M000009","%AAA%","정상적으로 저장을"),"black",true); 
}


function fn_D1_check(){
	var strSvcID        = "";
	var strPart         = "D10";	
	var strURL          = "handle.do?ServiceName=D1_RoadLeaseObjRegisMgrTax-service"
						+ "&BASE_YEAR=" + cob_BaseYear.Value
						;		
	
	var strInDatasets   = "";
	var strOutDatasets  = "ds_Check=ds_Check ds_Check1=ds_Check1";
	var strArgument     = "find_Check";
	 	 
	gfn_SyncCall( 
					 strSvcID
				   , strPart
				   , strURL
				   , strInDatasets
				   , strOutDatasets
				   , strArgument
			   );
}

//------------------------------------------------------------------------------- 
//   함수명: fn_D1_Find_A()
//   설명  :  과세표_A 내역 조회
//   arguments
//   return 내용: 없음
//------------------------------------------------------------------------------- 
function fn_D1_find_A()
{			    
    var strSvcID        = "";
	var strPart         = "D10";	
	var strURL          = "handle.do?ServiceName=D1_RoadLeaseObjRegisMgrTax-service"
						+ "&BASE_YEAR=" + cob_BaseYear.Value
						;
						
	var strInDatasets   = "";
	var strOutDatasets  = "ds_Tax_A=ds_Tax_A ds_Tax_B=ds_Tax_B ds_Tax_C=ds_Tax_C";
	var strArgument     = "find";
	 	 
	gfn_SyncCall( 
					 strSvcID	
				   , strPart
				   , strURL
				   , strInDatasets
				   , strOutDatasets
				   , strArgument
			   );
			   
	if(ds_Tax_A.GetRowCount()==0){
		Grid0.NoDataText = gfn_GetMessage("M000009","%AAA%","조회를");
	}
	
	if(ds_Tax_B.GetRowCount()==0){
		Grid1.NoDataText = gfn_GetMessage("M000009","%AAA%","조회를");
	}
}

//------------------------------------------------------------------------------- 
//   함수명: fn_D1_save_A()
//   설명  :  과세표_A 내역 저장
//   arguments
//   return 내용: 없음
//------------------------------------------------------------------------------- 
function fn_D1_save_A()
{   		
	ds_Tax_A.UpdateControl = false;
	
	if(ds_Check.GetColumn(0, "NCOUNT") > 0){
		for(var i=0; i<20; i++){
			ds_Tax_A.SetColumn(i, "BASE_YEAR", cob_BaseYear.Value);
			ds_Tax_A.SetRowType(i, "update");
			fn_GetDataSet_Value(i);
		}
	} else {
		for(var i=0; i<20; i++){
			ds_Tax_A.SetColumn(i, "BASE_YEAR", cob_BaseYear.Value);
			ds_Tax_A.SetRowType(i, "insert");
			fn_GetDataSet_Value(i);
		}
	}
	
	var strSvcID         = "";
	var strPart          = "D10";	
	var strURL           = "handle.do?ServiceName=D1_RoadLeaseObjRegisMgrTax-service";
	var strInDatasets    = "ds_Tax_A=ds_Tax_A:u";
	var strOutDatasets   = "";		
	var strArgument      = "save_A";
	 	 
	gfn_SyncCall( 
					 strSvcID
				   , strPart
				   , strURL
				   , strInDatasets
				   , strOutDatasets
				   , strArgument
			   );
}

function fn_D1_save_B()
{   
	ds_Tax_B.UpdateControl = false;
	
	var USER_ID = gfn_GetGlobalValue("G_USER_NO")  ;   // SESSION ID
	var USER_IP = gfn_GetGlobalValue("G_IPADDRESS");   // SESSION IP

	if(ds_Check1.GetColumn(0, "NCOUNT") > 0){
		for(var i=0; i<ds_Tax_B.GetRowCount(); i++){
			ds_Tax_B.SetRowType(i, "update");			
			ds_Tax_B.SetColumn(i, "BASE_YEAR", cob_BaseYear.Value);
			ds_Tax_B.SetColumn(i, "UPD_EMPID"  , USER_ID       );
			ds_Tax_B.SetColumn(i, "UPD_IP"     , USER_IP       );
			ds_Tax_B.SetColumn(i, "CRT_EMPID"  , USER_ID       );
			ds_Tax_B.SetColumn(i, "CRT_IP"     , USER_IP       );
		}
	} else {
		for(var i=0; i<ds_Tax_B.GetRowCount(); i++){
			ds_Tax_B.SetRowType(i, "insert");
			ds_Tax_B.SetColumn(i, "BASE_YEAR"  , cob_BaseYear.Value);
			ds_Tax_B.SetColumn(i, "UPD_EMPID"  , USER_ID       );
			ds_Tax_B.SetColumn(i, "UPD_IP"     , USER_IP       );
			ds_Tax_B.SetColumn(i, "CRT_EMPID"  , USER_ID       );
			ds_Tax_B.SetColumn(i, "CRT_IP"     , USER_IP       );
		}
	}	
	
	var strSvcID         = "";
	var strPart          = "D10";	
	var strURL           = "handle.do?ServiceName=D1_RoadLeaseObjRegisMgrTax-service";
	var strInDatasets    = "ds_Tax_B=ds_Tax_B:u";
	var strOutDatasets   = "";		
	var strArgument      = "save_B";
	 	 
	gfn_SyncCall( 
					 strSvcID
				   , strPart
				   , strURL
				   , strInDatasets
				   , strOutDatasets
				   , strArgument
			   );
}

function fn_D1_save_C()

{   					
	var USER_ID = gfn_GetGlobalValue("G_USER_NO")  ;   // SESSION ID
	var USER_IP = gfn_GetGlobalValue("G_IPADDRESS");   // SESSION IP
	
	ds_Tax_C.SetColumn(0, "BASE_YEAR"  , cob_BaseYear.Value);
	ds_Tax_C.SetColumn(0, "UPD_EMPID"  , USER_ID           );
	ds_Tax_C.SetColumn(0, "UPD_IP"     , USER_IP		   );
	ds_Tax_C.SetColumn(0, "CRT_EMPID"  , USER_ID           );
	ds_Tax_C.SetColumn(0, "CRT_IP"     , USER_IP		   );
	
	var strSvcID         = "";
	var strPart          = "D10";	
	var strURL           = "handle.do?ServiceName=D1_RoadLeaseObjRegisMgrTax-service";
	var strInDatasets    = "ds_Tax_C=ds_Tax_C:u";
	var strOutDatasets   = "";		
	var strArgument      = "save_C";
	 	 
	gfn_SyncCall( 
					 strSvcID
				   , strPart
				   , strURL
				   , strInDatasets
				   , strOutDatasets
				   , strArgument
			   );
}

//------------------------------------------------------------------------------- 
//   함수명: fn_GetDataSet_Value()
//   설명  :  도로상 점용과세표 등록 기본 셋팅
//   arguments
//   return 내용: 없음
//------------------------------------------------------------------------------- 
function fn_GetDataSet_Value(i){
			
	var USER_ID = gfn_GetGlobalValue("G_USER_NO")  ;   // SESSION ID
	var USER_IP = gfn_GetGlobalValue("G_IPADDRESS");   // SESSION IP

	ds_Tax_A.AddColumn("UPD_EMPID"     , "STRING", 256 );	// ID
	ds_Tax_A.AddColumn("UPD_IP"        , "STRING", 256 );	// IP
	ds_Tax_A.AddColumn("CRT_EMPID"     , "STRING", 256 );	// ID
	ds_Tax_A.AddColumn("CRT_IP"        , "STRING", 256 );	// IP
	
	ds_Tax_A.SetColumn(i, "UPD_EMPID"  , USER_ID       );
	ds_Tax_A.SetColumn(i, "UPD_IP"     , USER_IP	   );
	ds_Tax_A.SetColumn(i, "CRT_EMPID"  , USER_ID       );
	ds_Tax_A.SetColumn(i, "CRT_IP"     , USER_IP	   );
}

