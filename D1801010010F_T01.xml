function fn_CommonSave()
{	
	fn_D1_Save();
}

function fn_CommonAddRow(){
		
	if(ds_GvRmInst.GetRowCount() == 0){
		
		if(ds_GvRmInst_copy.GetRowCount() > 0){			
			ds_GvRmInst.Copy("ds_GvRmInst_copy");						
			ds_GvRmInst.UpdateControl = false;
			ds_GvRmInst.SetRowType(ds_GvRmInst.row, "update");		
		}else{		
			ds_GvRmInst.AddRow();			
		}
	}
}

//------------------------------------------------------------------------------- 
//   함수명: fn_D1_SAVE
//   설명  : 사유지 점용료 협의사항를 등록/업데이트한다.
//   arguments
//   return 내용: 없음
//------------------------------------------------------------------------------- 
function fn_D1_Save(value)
{	
	if(ds_GvRmInst.GetRowType(0) == "normal"){
		alert("등록 또는 변경 내용이 존재하지 않습니다. ");
		return false;
	}
	
	if(edt_DiscuYmd.Value == ''){
		alert("협의일자를 입력하세요. ");
		return false;
	}
	
	if(edt_DiscuFirmNm.Text == ''){
		alert("협의회사를 입력하세요. ");
		return false;
	}
	
	if(edt_DiscuTakerNm.Text == ''){
		alert("협의담당자 입력하세요. ");
		return false;
	}
	
	if(Radio.Value == '0'){
		if(edt_DocuReceiveYmd.Value == ''){
			alert("접수일자를 입력하세요. ");
			return false;
		}	
		
		if(edt_ReadYmd.Value == ''){
			alert("회람일자를 입력하세요. ");
			return false;
		}	
	}	
		
	var USER_ID    = gfn_GetGlobalValue("G_USER_NO");   // SESSION ID
	var USER_IP    = gfn_GetGlobalValue("G_IPADDRESS"); // SESSION IP

	// DB에 입력시킬 다음 값들을  셋팅 시켜 준다.
	ds_GvRmInst.SetColumn(0, "FACI_NUM"   , ds_GvRmView.GetColumn(0, "FACI_NUM"));
	ds_GvRmInst.SetColumn(0, "BRANCH_CD"  , ds_GvRmView.GetColumn(0, "BRANCH_CD"));
	
	ds_GvRmInst.SetColumn(0, "UPD_EMPID"  , USER_ID       );
	ds_GvRmInst.SetColumn(0, "UPD_IP"     , USER_IP		  );	
	ds_GvRmInst.SetColumn(0, "CRT_EMPID"  , USER_ID       );
	ds_GvRmInst.SetColumn(0, "CRT_IP"     , USER_IP		  );
						
	ds_GvRmInst.SetColumn(0, "AGREE_SQUARE", ds_GvRmView.GetColumn(ds_GvRmView.row, "AREA"));
	
	var strSvcID         = "";
	var strPart          = "D10";	
	var strURL           = "handle.do?ServiceName=D1_RentalDiscuRmkRegisMgr-service" ;					
	var strInDatasets    = "ds_GvRmInst=ds_GvRmInst:u";
	var strOutDatasets   = "";		
	var strArgument      = "save_Tab01";
	
	gfn_SyncCall( 
					 strSvcID
				   , strPart
				   , strURL
				   , strInDatasets
				   , strOutDatasets
				   , strArgument
			   );
			   	
	if(ErrorCode<0)
	{
		return false;
	}
			
	// history save
	ds_GvRmInst.UpdateControl = false;
	ds_GvRmInst.SetRowType(0, "insert");

	var strSvcID         = "";
	var strPart          = "D10";	
	var strURL           = "handle.do?ServiceName=D1_RentalDiscuRmkRegisMgr-service" ;					
	var strInDatasets    = "ds_GvRmInst=ds_GvRmInst:u";
	var strOutDatasets   = "";		
	var strArgument      = "his_Tab01";
	
	gfn_SyncCall( 
					 strSvcID
				   , strPart
				   , strURL
				   , strInDatasets
				   , strOutDatasets
				   , strArgument
			   );
	
	if(ErrorCode<0)
	{
		return false;
	}

	alert("정상적으로 저장을 완료하였습니다. ");
	return true;
}

