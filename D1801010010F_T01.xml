//------------------------------------------------------------------------------- 
//   함수명: fn_D1_Find()
//   설명  :  사유지 점용료 협의사항를 조회한다.
//   arguments
//   return 내용: 없음
//------------------------------------------------------------------------------- 
function fn_D1_Find(nRow, value, value1)
{	
    var strSvcID        = "fn_D1_Find";
	var strPart         = "D10";	
	var strURL          = "handle.do?ServiceName=D1_RentalDiscuRmkRegisMgr-service"
						+ "&FACI_NUM=" + value
						;
							
	var strInDatasets   = "";
	
	// 협의사항 DataSet & 협의의정보 DataSet	
	var strOutDatasets  = "ds_GvRmView=ds_GvRmView ds_GvRmInst=ds_GvRmInst ds_GvRmHis=ds_GvRmHis";
	var strArgument     = "view_Tab01";
	 
	gfn_SyncCall( 
					 strSvcID
				   , strPart
				   , strURL
				   , strInDatasets
				   , strOutDatasets
				   , strArgument
			   );
		
	if(ds_GvRmInst.GetColumn(0, "DOCU_RECEIVE_YMD") == '' || ds_GvRmInst.GetColumn(0, "DOCU_RECEIVE_YMD") == null){
		Radio.Value = 1;
		Radio_OnClick();
	}else{
		Radio.Value = 0;
		Radio_OnClick();
	}
	
	Button0.Enable = true;
	Button1.Enable = true;
	
	if(ds_GvRmInst.RowCount() == 0)
	{
		Radio.Value = 0;
		Radio_OnClick();
	}
		
	ds_GvRmInst_copy.ClearData();
	ds_GvRmInst_copy.Copy("ds_GvRmInst");
	
	if(ds_GvRmInst.GetColumn(0, "DISCU_YMD") == null || ds_GvRmInst.GetColumn(0, "DISCU_YMD") == ''){
		ds_GvRmInst.ClearData();
	}	
}

