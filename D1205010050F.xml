function fn_D1_ExcelPointList(nRow)
{
	ds_Multi_ExcelPoint.ClearData();

	ds_Search_ExcelPoint.ClearData();
	ds_Search_ExcelPoint.AddRow();
	
	ds_Search_ExcelPoint.SetColumn(0,"FACI_NUM"           , ds_Multi.GetColumn(nRow,"FACI_NUM")      );  
	ds_Search_ExcelPoint.SetColumn(0,"INPUT_FLAG"         , ds_Multi.GetColumn(nRow,"INPUT_FLAG")    );  
    ds_Search_ExcelPoint.SetColumn(0,"INPUT_YMD_FROM"      , cal_InputYmdFrom.Value     );  
    ds_Search_ExcelPoint.SetColumn(0,"INPUT_YMD_TO"        , cal_InputYmdTo.Value       );  

	
	var strSvcID = "find";
	var strPart = "D10";	
	var strURL = "handle.do?ServiceName=D1_PipeCoatingResisMgr-service";
 	var strInDatasets  = "ds_Search_ExcelPoint=ds_Search_ExcelPoint";
	var strOutDatasets = "ds_Multi_ExcelPoint=ds_Multi_ExcelPoint";
	var strArgument  = "searchExcelPoint";
			   	
	gfn_SyncCall( 
					 strSvcID
				   , strPart
				   , strURL
				   , strInDatasets
				   , strOutDatasets
				   , strArgument
			   );
	grd_ExcelPoint.NoDataText = gfn_GetMessage("M000022","%AAA%","조회결과가");
	
	var totalRow = ds_Multi_ExcelPoint.GetRowCount();
	
	for(var i=0;i<totalRow;i++){
		var delRow = ds_Multi_ExcelPoint.SearchRow("rowlevel=1 && SECTOR_NUM='배드점'",i);
		grd_ExcelPoint.ChangeRowHeight(delRow, 1, -1);
	}
	
}

