function open_Dialog(index)
{
	if(ds_Multi_Sector.GetColumn(0,"FACI_NUM")==null){
		alert("조회된 데이터만 그래프를 볼 수 있습니다.");
		return;
	}
	var args = "";
		args  = "FACI_NUM="    + quote(ds_Multi_Sector.GetColumn(0,"FACI_NUM")) ;
		args += " INDEX=" + quote(index);
		args += " YMDFROM=" + quote(cal_InputYmdFrom.Value);
		args += " YMDTO=" + quote(cal_InputYmdTo.Value);
	gfn_Dialog("D10","D1205010051U",args,1270,853,true,-1,-1);
}

