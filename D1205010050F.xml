function cob_Branchcd_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex) 
{
    fn_SetComboBox_Comcode("1", cob_Branchcd.value, fn_setDsObject_Comcode("1", "cob_ZipNo1"));
    cob_ZipNo1.Value = "";  
    gfn_D1_SetComboBox_Teamcd("ds_Team",cob_Branchcd);
}

function btn_RecDraNmSearch_OnClick(obj)
{
	var retValue = gfn_Dialog("D10","D1205010050U","",450,400,true,-1,-1);
	if(retValue != "" && retValue != null)
		{
			edt_FaciNum.Text = retValue[0]; 
			if(retValue[1] != null){
				edt_RecDraNm.Text = retValue[1];
			}
			if(retValue[2] != null){
				edt_RecDraNm.Text = retValue[2];
			}
			
		}
}

