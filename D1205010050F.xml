	var strSvcID = "find";
	var strPart = "D10";	
	var strURL = "handle.do?ServiceName=D1_PipeCoatingResisMgr-service";
 	var strInDatasets  = "ds_Search_Sector=ds_Search_Sector";
	var strOutDatasets = "ds_Multi_Sector=ds_Multi_Sector";
