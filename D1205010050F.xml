function D1205010050F_OnLoadCompleted(obj)
{
	gfn_LoadForm(obj);
	gfn_SetButton("00010101");
	fn_Init();
}

function fn_Init()
{
	gfn_D1_All_SetComboBox_Branchcd(gfn_D1_setDsObject_Branchcd("cob_Branchcd"));			//지사코드(전체포함)
	cob_BranchCd.Enable = true;
	cob_BranchCd.Value = "";
	cob_BranchCd_OnChanged();
	
	//TEST
	cal_InputYmdFrom.Value = substr(AddMonth(today(),-1),0,6) + '01';
	cal_InputYmdTo.Value = today();
}

function fn_CommonSearch()
{
	fn_D1_List();
	grd_Mainter_OnCellClick();
}

function fn_D1_List()
{
	ds_Multi.ClearData();

	ds_SearchTop.ClearData();
	ds_SearchTop.AddRow();
	
    ds_SearchTop.SetColumn(0,"BRANCH_CD"          , cob_Branchcd.Value        );  // 지사
    ds_SearchTop.SetColumn(0,"ZIP_NO1"            , cob_ZipNo1.Value          );  // 시/구
	ds_SearchTop.SetColumn(0,"RECTIFIER_NM"       , edt_RecDraNm.Text         );  //정배류기명
	ds_SearchTop.SetColumn(0,"DRAINAGE_NM"        , edt_RecDraNm.Text         );  //정배류기명
	ds_SearchTop.SetColumn(0,"FACI_NUM"           , edt_FaciNum.Text          );  //시설물 번호
    ds_SearchTop.SetColumn(0,"INPUT_YMD_FROM"      , cal_InputYmdFrom.Value     );  // 작업요청일자
    ds_SearchTop.SetColumn(0,"INPUT_YMD_TO"        , cal_InputYmdTo.Value       );  // 작업요청일자

	
	var strSvcID = "find";
	var strPart = "D10";	
	var strURL = "handle.do?ServiceName=D1_PipeCoatingResisMgr-service";
 	var strInDatasets  = "ds_SearchTop=ds_SearchTop";
	var strOutDatasets = "ds_Multi=ds_Multi";
	var strArgument  = "search";
		
			   	
	gfn_SyncCall( 
					 strSvcID
				   , strPart
				   , strURL
				   , strInDatasets
				   , strOutDatasets
				   , strArgument
			   );
	grd_Mainter.NoDataText = gfn_GetMessage("M000022","%AAA%","조회결과가");	
}

