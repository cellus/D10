// 2013-07-02 김형수 키값에 따른 판정값 자동 변환 
function div_Info_msk_SoilPh_OnKillFocus(obj)
{
    var coatResis = currency(obj.value);
        
    if(coatResis >= 2001)
    {
        div_Info.cob_judeRslt.value = "10"; // 매우양호
    }
    else if(coatResis >= 1001 && coatResis <= 2000)
    {
        div_Info.cob_judeRslt.value = "20"; // 양호
    }
    else if(coatResis >= 200 && coatResis <= 1000)
    {
        div_Info.cob_judeRslt.value = "30"; // 보통
    }
    else if(coatResis >= 26 && coatResis <= 199)
    {
        div_Info.cob_judeRslt.value = "40"; // 불량
    }
    else if(coatResis <= 25)
    {
        div_Info.cob_judeRslt.value = "50"; // 매우불량
    }	
}
