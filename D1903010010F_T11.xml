function chkLengthSubStr(id, strPreText , limitLength){

		// 길이 체크
	var size = 0;
	var chkLength = 0;
  
   for(var i=0; i < Length(strPreText); i++){    
		chkLength ++;
		if(Lengthb(substr(strPreText,i,1)) == 2){
			size += 3;
		}else{
			size += 1;
		}   
		
		if(size > limitLength){
			break;
		}
   }
	
	ds_sector.SetColumn(ds_sector.currow, id, SubStr(strPreText,0, chkLength-1) );

}

